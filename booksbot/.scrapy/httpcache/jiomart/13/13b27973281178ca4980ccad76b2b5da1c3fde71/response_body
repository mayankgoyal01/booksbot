$(window).on('load', function(){
if(localStorage.getItem('jio_vertical_city_code')){
        var master_code = JSON.parse(localStorage.getItem('jio_vertical_city_code'));
        var default_master_code = window.default_vertical_city_code;

        var master_length = Object.keys(master_code).length;
        var def_master_length = Object.keys(default_master_code).length;

        if(master_length < def_master_length){
            var pincode = localStorage.getItem('nms_mgo_pincode');
            getCityDetails(pincode,true,false,false,false);
        }
    }
});
